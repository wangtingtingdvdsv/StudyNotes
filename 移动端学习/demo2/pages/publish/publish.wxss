.publisHomework{
  color: white;
  width: 100%;
  position: fixed;
  bottom: 0;
  height: 100rpx;
  background-color: #7CD6D6;
}
.delete{
  display: none;
}
.pic image{
  width: 205rpx;
  height: 205rpx;
  margin-right: 5rpx;
  position: relative;
}
.pic{
  display: flex;
  align-items: center;
}
.homework{
  position: absolute;
  bottom: 0;
}
.switch{
  position: absolute;
  right: 0;
  bottom: 0;
}
.loadButt{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 200rpx;
  height: 200rpx;
}
.loadIcon{
  width: 10rpx;
  height: 10rpx;
}
.hide{
  display: none;
}
.pic{
  margin-top: 100rpx;
  border: 1px dashed black;
  height: 200rpx;
  margin-right: 20rpx;
  position: absolute;
  left: 50%;
  margin-left: -45%;

}
.loadPic{
  background-color: #F0EAEA;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-top: 30rpx;
  height: 370rpx;
  border-bottom: 1rpx solid black;
  border-top: 1px solid black;
}
.picker{
  width: 100%;
}
.title{
  background-color: #F0EAEA;
  height: 100rpx;
  border-bottom: 1rpx solid black;
  border-top: 1px solid black;
}
.tui-picker-content{
  background-color: #F0EAEA;
  display: flex;
  align-items: center;
  margin-top: 30rpx;
  height: 100rpx;
  border-bottom: 1rpx solid black;
  border-top: 1px solid black;
}
.speak{
  background-color: #F0EAEA;
  width: 100%;
  margin-top: 30rpx;
  height: 400rpx;
  border-bottom: 1rpx solid black;
  border-top: 1px solid black;
}